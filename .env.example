# AEOS SOAP Web Services
AEOS_WSDL_URL=https://your-aeos-server:8443/aeosws?wsdl
AEOS_WS_USER=your_ws_user
AEOS_WS_PASSWORD=your_ws_password
AEOS_WS_VERIFY_SSL=false

# SQL Server (for analytics view vw_AeosEventLog)
DB_SERVER=your-sql-server
DB_NAME=aeosdb
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_TRUSTED_CONNECTION=no

# Application
SECRET_KEY=change-me-in-production
POLL_INTERVAL_SECONDS=5
HOST=0.0.0.0
PORT=5000
FLASK_DEBUG=false
